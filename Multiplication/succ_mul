induction b with d hd,
repeat {rw mul_zero},
rwa [add_zero],
rw succ_eq_add_one (d),
rw mul_add a d 1,
rw ← succ_eq_add_one (d),
rw mul_one,
rw add_comm (a * d) a,
rw add_right_comm (a) (a * d) (succ d),
rw add_succ,
rw add_comm a d,
rw ← add_succ,
rw ← add_right_comm d (a * d) (succ a),
rw ← add_comm (a * d) d,
rw ← hd,
rw ← mul_succ,
refl,
